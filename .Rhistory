n_proteina_alimento_dieta_cereales1<- as.numeric(proteina_alimento_dieta_cereales1)
proteina_totales_cereales1<- (n_proteina_alimento_dieta_cereales1*n_porcion_cereales1)/100
#Saca los carbohidratos
